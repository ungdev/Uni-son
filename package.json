        "v-tooltip": "2.0.0-rc.33",
        "vue": "2.6.6",
        "vue-carousel": "0.17.0",
        "vue-clickaway": "2.2.2",
        "vue-easy-lightbox": "^0.3.10",
        "vue-eslint-parser": "^6.0.3",
        "vue-gallery": "^2.0.0",
        "vue-gallery-slideshow": "^1.3.1",
        "vue-lazyload": "1.2.6",
        "vue-material": "1.0.0-beta-10.2",
        "vue-router": "3.0.2",
